<!--pages/mineSub/details/complete/complete.wxml-->
<view class="page">
  <view class="complete-background weui-cells_card">
    <form bindsubmit="bindSubmit">
      联系电话：
      <input type="number" maxlength="{{11}}" placeholder="请输入电话" name="mobile"></input>
      电子邮箱：
      <input placeholder="请输入邮箱" name="email"></input>
      <text>\n</text>
      <button form-type="submit">提交</button>
    </form>
  </view>
</view>